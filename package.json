{
    "name": "@womens-business-club/wbc-components",
    "version": "0.2.34",
    "main": "dist/index.js",
    "private": false,
    "files": [
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/womens-business-club/wbc-components"
    },
    "dependencies": {
        "@storybook/preset-scss": "^1.0.3",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "bootstrap": "^4.6.0",
        "css-loader": "^4.3.0",
        "node-sass": "^6.0.1",
        "react-bootstrap": "^1.6.1",
        "react-helmet": "^6.1.0",
        "sass-loader": "^10.2.0",
        "style-loader": "^1.3.0",
        "web-vitals": "^1.1.2"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "deploy-storybook": "vercel --prod",
        "clean": "rimraf dist",
        "compile": "npm run clean && cross-env NODE_ENV=production babel src/components src/scss --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
        "deploy": "npm run compile && git add . && git commit -am 'release' && npm version patch && git push"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/preset-env": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "@storybook/addon-actions": "^6.3.4",
        "@storybook/addon-essentials": "^6.3.4",
        "@storybook/addon-links": "^6.3.4",
        "@storybook/node-logger": "^6.3.4",
        "@storybook/preset-create-react-app": "^3.2.0",
        "@storybook/react": "^6.3.4",
        "cross-env": "^7.0.3"
    },
    "publishConfig": {
        "@womens-business-club:registry": "https://npm.pkg.github.com"
    }
}
